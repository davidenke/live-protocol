{
  "name": "live-protocol",
  "version": "0.5.2",
  "description": "An application to generate protocol documents from mind maps.",
  "bin": "dist/index.js",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "tauri icon src/assets/icons/app.icon.png",
    "lint": "eslint ./src",
    "test": "vitest",
    "dev": "tauri dev",
    "dev:detached": "vite dev --mode detached",
    "build": "tauri build"
  },
  "author": {
    "email": "david@enke.dev",
    "name": "David Enke"
  },
  "dependencies": {
    "@fontsource-variable/inter-tight": "5.2.7",
    "@fontsource-variable/material-symbols-outlined": "5.2.30",
    "@tauri-apps/api": "2.9.1",
    "@tauri-apps/plugin-dialog": "2.4.2",
    "@tauri-apps/plugin-fs": "2.4.4",
    "@tauri-apps/plugin-shell": "2.3.3",
    "@turbodocx/html-to-docx": "1.10.0",
    "jszip": "3.10.1",
    "lit": "3.3.1",
    "marked": "17.0.1",
    "xmind-model": "1.1.12"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "1.9.0",
    "@enke.dev/lint": "0.11.9",
    "@tauri-apps/cli": "2.9.6",
    "@types/eslint__eslintrc": "3.3.0",
    "@types/eslint__js": "9.14.0",
    "@types/node": "25.0.0",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "autoprefixer": "10.4.22",
    "buffer": "6.0.3",
    "eslint": "9.39.1",
    "fast-glob": "3.3.3",
    "jiti": "2.6.1",
    "postcss": "8.5.6",
    "postject": "1.0.0-alpha.6",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vite": "7.2.7",
    "vite-plugin-checker": "0.12.0",
    "vite-plugin-node-polyfills": "0.24.0",
    "vitest": "4.0.15"
  },
  "bugs": "https://github.com/davidenke/live-protocol/issues",
  "homepage": "https://github.com/davidenke/live-protocol#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidenke/live-protocol.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "patchedDependencies": {
      "eslint-plugin-simple-import-sort": "patches/eslint-plugin-simple-import-sort.patch"
    },
    "onlyBuiltDependencies": [
      "@turbodocx/html-to-docx",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
